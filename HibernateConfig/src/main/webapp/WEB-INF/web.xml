<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>InitializeHibernates</servlet-name>
		<display-name>InitializeHibernates</display-name>
		<description></description>
		<servlet-class>com.simplilearn.InitializeHibernates</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListProducts</servlet-name>
		<display-name>ListProducts</display-name>
		<description></description>
		<servlet-class>com.simplilearn.ListProducts</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListEmployees</servlet-name>
		<display-name>ListEmployees</display-name>
		<description></description>
		<servlet-class>com.simplilearn.ListEmployees</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InitializeHibernates</servlet-name>
		<url-pattern>/initializeHibernates</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListProducts</servlet-name>
		<url-pattern>/listProducts</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListEmployees</servlet-name>
		<url-pattern>/listEmployees</url-pattern>
	</servlet-mapping>
</web-app>
